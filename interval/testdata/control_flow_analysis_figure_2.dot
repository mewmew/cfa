// Derived from figure 2 in F. Allen, "Control Flow Analysis", 1970.
//
// http://www.cs.columbia.edu/~suman/secure_sw_devel/p1-allen.pdf

digraph G {
	// Nodes.
	1 [label=entry];
	2;
	3;
	4;
	5;
	6;
	7;
	8;

	// Edges.
	1 -> 2
	2 -> 3
	2 -> 7
	3 -> 4
	3 -> 5
	4 -> 3
	4 -> 6
	5 -> 6
	6 -> 7
	7 -> 2
	7 -> 8
}
